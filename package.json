{
    "name": "windfish",
    "version": "4.0.2",
    "description": "Windfish.io",
    "author": "Eric Lakatos",
    "private": true,
    "engines": {
        "node": ">=10.0.0"
    },
    "scripts": {
        "dev": "nodemon -w nuxt.config.js server.js",
        "dev:settings": "FLUSH_SETTINGS=true yarn run dev",
        "build": "nuxt build",
        "start": "node server.js",
        "start-prod-mock": "PRECOMMIT=true yarn run heroku-mock",
        "heroku-mock": " yarn run heroku-postbuild && NODE_ENV=production  yarn run start",
        "heroku-postbuild": "nuxt build",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
        "precommit": " yarn run lint &&  yarn run start-prod-mock"
    },
    "dependencies": {
        "@nuxtjs/google-analytics": "^2.0.2",
        "body-parser": "^1.18.3",
        "colors": "^1.3.0",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "jwt-decode": "^2.2.0",
        "node-fetch": "^2.1.2",
        "nuxt": "^1.4.0",
        "redirect-ssl": "^1.3.0",
        "socket.io": "^2.1.1",
        "socket.io-client": "^2.1.1",
        "tmi.js": "^1.2.1"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.1",
        "eslint": "^4.15.0",
        "eslint-friendly-formatter": "^3.0.0",
        "eslint-loader": "^1.7.1",
        "eslint-plugin-vue": "^4.0.0",
        "husky": "^0.14.3",
        "node-sass": "^4.9.0",
        "nodemon": "^1.17.5",
        "pug": "^2.0.0-beta6",
        "pug-loader": "^2.4.0",
        "sass-loader": "^7.0.1"
    }
}
